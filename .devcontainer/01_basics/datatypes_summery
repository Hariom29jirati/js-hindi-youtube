// stack and heap memory

/*

#  stack memory is used by primitive datatypes
   in this copy of memory is given

#  heap memory is used by non-primitive datatypes 
      in this reference of memory is given

*/

//eg of stack
    let one = 1;
    let two = one
    console.log(one)
    console.log(two)


// eg of heap

let userOne = {
    email: "hitesh@google.com"
}

let userTwo = userOne
console.log([userOne, userTwo])

userTwo.email = "hariom@google.com"
console.log([userOne, userTwo])